      <template src="Proxy/Missing_Proxies_Template.png" default="True">
         <matches>
            <match name="Name" value="Whatever" />
         </matches>
         <textblocks>
            <link block="default_name" separator=" ">
               <property name="Name"  />
            </link>
         </textblocks>
      </template>
   <!-- Outfit Template -->
      <template src="Proxy/blank_background.jpg">
         <matches>
            <match name="Type" value="Outfit" />
         </matches>
         <overlayblocks>
            <conditional>
               <switch property="Outfit">
                  <case value="The Agency">
                     <link block="outfit_agency" />
                     <link block="home_outfit_icon_agency" />
                  </case>
                  <case value="Blackjacks">
                     <link block="outfit_blackjacks" />
                     <link block="home_outfit_icon_blackjacks" />
                  </case>
                  <case value="Collegium">
                     <link block="outfit_collegium" />
                     <link block="home_outfit_icon_collegium" />
                  </case>
                  <case value="Law Dogs">
                     <link block="outfit_law_dogs" />
                     <link block="home_outfit_icon_law_dogs" />
                  </case>
                  <case value="Lost Angels">
                     <link block="outfit_lost_angels" />
                     <link block="home_outfit_icon_lost_angels" />
                  </case>
                  <case value="Maze Rats">
                     <link block="outfit_maze_rats" />
                     <link block="home_outfit_icon_maze_rats" />
                  </case>
                  <case value="Sioux Union">
                     <link block="outfit_sioux" />
                     <link block="home_outfit_icon_sioux" />
                  </case>
                  <case value="Sweetrock">
                     <link block="outfit_sweetrock" />
                     <link block="home_outfit_icon_sweetrock" />
                  </case>
                  <case value="Texas Rangers">
                     <link block="outfit_texas_rangers" />
                     <link block="home_outfit_icon_texas_rangers" />
                  </case>
                  <case value="Flock">
                     <link block="outfit_flock" />
                     <link block="home_outfit_icon_flock" />
                  </case>
                  <case value="Whateleys">
                     <link block="outfit_whateleys" />
                     <link block="home_outfit_icon_whateleys" />
                  </case>
                  <case value="Coalition">
                     <link block="outfit_coalition" />
                     <link block="home_outfit_icon_texas_rangers" />
                     <link block="home_outfit_icon_agency_coalition" />
                  </case>
               </switch>
            </conditional>
         </overlayblocks>
         <textblocks>
            <link block="outfit_name" separator=" ">
               <property name="Name"  />
            </link>
            <link block="starting_rock" separator=" ">
               <property name="Ghost Rock"  />
            </link>
            <link block="outfit_text" separator=" ">
               <property name="Text"  />
            </link>
            <link block="production_upkeep">
               <property name="Production" format="+{}" />
            </link>
         </textblocks>
      </template>   
